---
import { Code } from '@astrojs/starlight/components';

interface Props {
  code: string;
  lang: string;
  meta?: string;
  locale?: string;
  title?: string;
  frame?: 'auto';
  mark?: string;
}

const { code, lang, meta, locale, title, frame, mark } = Astro.props;
---

<Code 
  code={code}
  lang={lang}
  meta={meta}
  locale={locale}
  title={title}
  frame={frame}
  mark={mark}
/>
